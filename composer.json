{
    "name": "yuriystank/rules",
    "type": "library",
    "description": "",
    "keywords": [
        "yuriystank",
        "rules"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "yuriystank",
            "email": "yuriystank@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "yuriystank\\rules\\": "src"
        }
    },
    "autoload-dev": {
